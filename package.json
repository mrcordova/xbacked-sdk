{
  "name": "@xbacked-dao/xbacked-sdk",
  "version": "0.0.1",
  "packageManager": "yarn@3.1.0",
  "description": "Official SDK for the xBacked Protocol",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/browser/xbacked-sdk.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "directories": {
    "test": "src/__tests__",
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xBacked-DAO/xbacked-sdk.git"
  },
  "devDependencies": {
    "@babel/types": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "jest-environment-jsdom": "^27.0.6",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@randlabs/myalgo-connect": "^1.1.1",
    "@reach-sh/stdlib": "^0.1.7-rc.0",
    "@xbacked-dao/xbacked-contracts": "0.0.3"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "yarn run build-esm & yarn run build-cjs",
    "build-esm": "tsc -p ./tsconfig-esm.json",
    "build-cjs": "tsc -p ./tsconfig-cjs.json",
    "build-browser": "webpack --config webpack.config.js",
    "clean": "rm -rf dist/",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint"
  },
  "keywords": [],
  "author": "xBacked",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xBacked-DAO/xbacked-sdk/issues"
  },
  "homepage": "https://github.com/xBacked-DAO/xbacked-sdk#readme"
}

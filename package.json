{
  "name": "xbacked-sdk",
  "version": "1.0.0",
  "description": "Official SDK for the xBacked Protocol",
  "main": "lib/index.js",
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier \"src/**/*.ts\" ",
    "lint": "tslint -p tsconfig.json",
    "devnet": "REACH_CONNECTOR_MODE=ALGO ./src/reach devnet --await-background",
    "reach:down": "./src/reach down",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xBacked-DAO/xbacked-sdk.git"
  },
  "keywords": [],
  "author": "Xbacked",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xBacked-DAO/xbacked-sdk/issues"
  },
  "homepage": "https://github.com/xBacked-DAO/xbacked-sdk#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@babel/types": "^7.16.0",
    "@reach-sh/stdlib": "^0.1.7-rc.0",
    "@types/node": "^16.11.9"
  }
}
